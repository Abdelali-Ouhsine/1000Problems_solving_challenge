
#include <stdio.h>
#include <stdbool.h>

int main()
{
	int age;
	int drive;
	int result;

	//read age and druverlicense
	printf("enter your age");
        scanf("%d",&age);
	printf("if you have a driver license enter true");
	scanf("%d",&drive);

	result = (age >= 21 && drive == true);
	if(result == true)
	{
		printf("Hired");
	}
	else
		printf("Rejected");
}
